<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/lebron"
    tools:context=".ui.athletics.AthleticsFragment">

    <TextView
        android:id="@+id/text_has_worked_out"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="89dp"
        android:layout_marginBottom="615dp"
        android:fontFamily="sans-serif-black"
        android:text="ATHELTIC TRAINING"
        android:textAlignment="center"
        android:textColor="#00FFC3"
        android:textSize="32sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/text_home_workout_counter"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="352dp"
        android:layout_marginBottom="352dp"
        android:fontFamily="sans-serif-medium"
        android:text="@string/info_Athletics"
        android:textAlignment="center"
        android:textColor="#00FFC3"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:text="@string/info_Athletics2"
        android:textAlignment="center"
        android:textColor="#00FFC3"
        android:textSize="22sp"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="401dp" />


</androidx.constraintlayout.widget.ConstraintLayout>